    <a-link href="https://kwonjo.github.io/visualization_interactivegraphics/sketch6.html" title="Jo" width="4" height="3"  rotation="0 0 0" position="11 2 -4.45" image="#jo"></a-link>
    <a-link href="https://bobobocuz.github.io/artsvis_visualization/" title="Eric" width="4" height="3"  rotation="0 0 0" position="5 2 -4.45" image="#eric"></a-link>
    <a-link href="https://cassidybolt.github.io/Cassidy-s-Data-Visualization/" title="Cassidy" width="4" height="3"  rotation="0 0 0" position="2 2 -4.45" image="#cassidy"></a-link>
    <!--left wall-->
    <a-link href="https://geli25.github.io/Assignment3/"  title="Angelina" width="4" height="3" rotation="0 90 0" position="0.8 2 3" image="#angelina"></a-link>
    <a-link href="https://kiraxie17.github.io/DataVisualization_version1/"  title="Kira" width="4" height="3" rotation="0 90 0" position="0.8 2 0" image="#kira"></a-link>
    <a-link href="https://geli25.github.io/Assignment3/"  title="Paschalia" width="4" height="3" rotation="0 90 0" position="0.8 2 -3" image="#angelina"></a-link>
    

    <!-- Pedestals -->
    <a-box  href="http://aframe.io/"  static-body color="#FFF" width="0.6" height="2" width="0.6" position="10 0 0"></a-box >
    <a-box  href="http://aframe.io/"  static-body color="#FFF" width="0.8" height="2" width="0.8" position="8 0 4"></a-box >
    <a-box  href="http://aframe.io/"  static-body color="#FFF" width="0.8" height="2" width="0.8" position="4 0 -4"></a-box >
</a-scene>

