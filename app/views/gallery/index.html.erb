   <a-entity position="6 0 0">
        <a-camera id="camera" look-controls wasd-controls>
          <a-cursor color="#4CC3D9"></a-cursor>
          <a-ring radius-outer="0.30" radius-inner="0.20"
            position="0 0 -10"
            material="color: cyan; shader: flat"
            cursor="maxDistance: 30; fuse: true">
          </a-ring>
        </a-camera>
      </a-entity>

    <!-- Sky -->
    <a-sky src="#skybox" rotation="0 -130 0"></a-sky>

    <!-- Ground -->
    <a-plane  static-body src="#concrete" height="12" width="20" rotation="-90 0 0" position="9.3 0 0"></a-plane>

    <!--<a-box  static-body src="#wood" height="14" width="6" rotation="-90 0 0" position="7 5 0"></a-box>-->

    <!-- Walls -->
    <a-box  color="#FFF" width="15" height="5" depth="1" position="8 2.5 -5" static-body ></a-box>
    <a-box  color="#FFF" width="15" height="5" depth="1" position="8 2.5 5" static-body></a-box>
    <a-box  color="#FFF" width="10" height="5" depth="1" rotation="0 90 0" position="14 2.5 0" static-body></a-box>
    <a-box  color="#FFF" width="10" height="5" depth="1" rotation="0 90 0" position="0 2.5 0" static-body></a-box>

     <!-- Pedestals -->
    <a-box  static-body color="#FFF" width="0.6" height="2" width="0.6" position="10 0 0"></a-box >

    <!-- Menu -->
    <a-text href="https://dukeinteractivegraphics.herokuapp.com/landing/about" value="About the Artists" size="30" width="0.3" height="0.4" color="black" rotation="270 0 270" position="10 1.08 -0.2"></a-text>
    <a-box  href="https://dukeinteractivegraphics.herokuapp.com/landing/about" color="#FFF" width="0.08" height="0.05" depth="0.2" position="10 1.05 -0.15" static-body ></a-box>
